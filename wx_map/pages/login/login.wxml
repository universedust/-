<!--pages/login/login.wxml-->
<view class="container"> 
 <view class="login-icon"> 
 <image class="login-img" src="/images/back.png"></image> 
 </view> 
 <view class="login-from"> 
  
 <!--经度坐标-->
 <view class="inputView"> 
 <image class="nameImage" src="/images/map.png"></image> 
 <label class="loginLab">个人经度坐标</label> 
 <input class="inputText" placeholder="请输入个人经度坐标" name="x" bindinput="nameInput" /> 
 </view> 
 <view class="line"></view> 
  
 <!--纬度坐标-->
 <view class="inputView"> 
 <image class="keyImage" src="/images/map.png"></image> 
 <label class="loginLab">个人纬度坐标</label> 
 <input class="inputText" placeholder="请输入个人纬度坐标" name="y" bindinput="passwordInput" /> 
 </view> 
  
 <!--按钮-->
 <view class="loginBtnView"> 
 <button class="loginBtn" type="primary" bindtap="denglu">开始寻物</button> 
 </view> 
 <!-- <view class="loginBtnView"> 
 <button class="loginBtn" type="primary" bindtap="toregister">用户注册</button> 
 </view>  -->
 </view> 
</view>
